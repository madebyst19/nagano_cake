<div class="row">
　 <h3>商品一覧</h3><%= link_to"add",new_admins_item_path,class:"fas fa-plus-circle" %>


         <table class="table">
            <thead><tr><th>商品ID</th><th>商品名</th><th>税抜価格</th><th>ジャンル</th><th>ステータス</th></thead>
                  <% @items.each do |item|  %>
                    <tr>
                        <td><%=item.id %></td>
                        <td><%= item.name %></td>
                        <td><%=item.price %></td>
                        <td><%= item.genre %></td>
                        <td><%#= item.is_active %></td>
                    </tr>
                  <% end %>
          </table>
</div>